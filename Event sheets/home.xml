<?xml version="1.0" encoding="utf-8" ?>
<c2eventsheet>
    <!--All the 'name' attributes are ignored by Construct 2 - they are there for readability only.-->
    <name>home</name>
    <events>
        <event-block>
            <conditions>
                <condition id="0" name="Clay.io Ready" type="Clayio" />
            </conditions>
            <actions>
                <action id="-16" name="Set opacity" type="button_play">
                    <param id="0" name="Opacity">100</param>
                </action>
            </actions>
        </event-block>
        <event-block>
            <conditions>
                <condition id="4" name="Is touching object" type="Touch">
                    <param id="0" name="Object">button_play</param>
                </condition>
            </conditions>
            <actions>
                <action id="-25" name="Go to layout (by name)" type="System">
                    <param id="0" name="Layout">&quot;cup_01&quot;</param>
                </action>
            </actions>
        </event-block>
    </events>
</c2eventsheet>
